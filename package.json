  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thunder-solutions/thunder-events/issues"
  },
  "homepage": "https://github.com/thunder-solutions/thunder-events#readme",
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/babel__core": "^7.1.18",
    "express": "^4.17.3",
    "jest": "^27.5.1",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.68.0",
    "rollup-jest": "^1.1.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "dialog-polyfill": "^0.5.6"
  }
}
