{
  "name": "@thundersolutions/events",
  "version": "0.0.1",
  "description": "A package for all event planning needs.",
  "main": "dist/thunderEvents.min.js",
  "module": "esm/thunderEvents.min.mjs",
  "exports": {
    ".": {
      "node": "./umd/thunderEvents.min.js",
      "import": "./esm/thunderEvents.min.mjs",
      "require": "./dist/thunderEvents.min.cjs"
    },
    "./components": {
      "node": "./umd/components/index.min.js",
      "import": "./esm/components/index.min.mjs",
      "require": "./dist/components/index.min.cjs"
    },
    "./components/": {
      "node": "./umd/components/",
      "import": "./esm/components/",
      "require": "./dist/components/"
    }
  },
  "scripts": {
    "build": "rollup --config --exports default",
    "start": "node demoServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thunder-solutions/thunder-events.git"
  },
